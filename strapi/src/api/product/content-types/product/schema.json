{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "stock_available": {
      "type": "integer",
      "default": 0
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "barcode": {
      "type": "string"
    },
    "statut": {
      "type": "enumeration",
      "enum": [
        "safe",
        "danger",
        "empty"
      ]
    }
  }
}
